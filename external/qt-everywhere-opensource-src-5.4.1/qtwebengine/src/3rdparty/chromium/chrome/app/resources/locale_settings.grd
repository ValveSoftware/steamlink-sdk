          60
        </message>
        <message name="IDS_JOIN_WIFI_NETWORK_DIALOG_MINIMUM_HEIGHT_LINES" use_name_for_id="true">
          4
        </message>

        <!-- The height of the advanced Join Wi-Fi network dialog box in lines. -->
