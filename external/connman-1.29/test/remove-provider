#!/usr/bin/python

import sys
import dbus

if (len(sys.argv) < 2):
	print "Usage: %s <VPN service path> " % (sys.argv[0])
	sys.exit(1)

bus = dbus.SystemBus()

manager = dbus.Interface(bus.get_object("net.connman", "/"),
					"net.connman.Manager")

path = "" + sys.argv[1]

print "remove path is %s" %(path)

manager.RemoveProvider(sys.argv[1])
