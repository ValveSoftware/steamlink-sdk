[
    RuntimeEnabled=Sensor,
    Constructor(optional SensorOptions sensorOptions),
    ConstructorCallWith=ScriptState,
    RaisesException=Constructor,
] interface Gyroscope : Sensor {
    readonly attribute GyroscopeReading? reading;
};
